{
  "name": "@brainsait/healthcare-sdk",
  "version": "1.2.0",
  "description": "BrainSAIT Healthcare SDK for NPHIES/FHIR Integration with Arabic/HIPAA support and AI agents",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "build:pages": "./scripts/build-pages.sh",
    "build:types": "tsc --emitDeclarationOnly",
    "build:worker": "tsc -p tsconfig.worker.json",
    "build:docs": "vitepress build docs",
    "build:all": "npm run build && npm run build:worker && npm run build:docs",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "benchmark": "npm run build && node benchmarks/simple-benchmark.js",
  "deploy:worker": "wrangler deploy --config wrangler-worker.toml",
  "deploy:pages": "wrangler pages deploy public --config wrangler.toml",
  "deploy:cf": "./scripts/deploy-cloudflare.sh full production",
  "cf:dev": "wrangler dev --config wrangler-worker.toml",
  "cf:tail": "wrangler tail --config wrangler-worker.toml",
    "preview": "vite preview",
    "clean": "rm -rf dist coverage public docs/.vitepress/dist",
    "validate": "npm run typecheck && npm run lint:check && npm run format:check && npm run test:ci"
  },
  "keywords": [
    "healthcare",
    "fhir",
    "nphies",
    "saudi-arabia",
    "hipaa",
    "ai-agents",
    "arabic",
    "glass-morphism",
    "sdk"
  ],
  "author": "BrainSAIT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fadil369/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Fadil369/sdk/issues"
  },
  "homepage": "https://github.com/Fadil369/sdk#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250927.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.3.24",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^0.1.1",
    "vitest": "^0.34.6",
    "wrangler": "^4.40.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "i18next": "^23.5.1",
    "joi": "^17.10.2",
    "lodash": "^4.17.21",
    "pino": "^8.15.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
